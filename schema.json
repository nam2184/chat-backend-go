  "openapi": "3.0.0",
  "paths": {
    "/auth": {
      "post": {
        "operationId": "PostAuth",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PostAuthResponse"
                }
              }
            },
            "description": "Authentication success"
          },
          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            },
            "description": "Authentication success"
          },
          "default": {
            "description": ""
          }
        },
        "summary": "Authenticate user and return tokens"
      }
    },
    "/auth/refresh": {
      "get": {
        "operationId": "GetRefreshToken",
