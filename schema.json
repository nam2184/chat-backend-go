          "400": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorResponse"
                }
              }
            },
            "description": "Refresh Authentication success"
          },
          "default": {
            "description": ""
          }
        },
        "summary": "Refresh access token"
      }
    },
